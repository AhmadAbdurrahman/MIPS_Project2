# program: 1000 char string
# N = 31
# M = 21

.data
user_string: .space 1001
error: .asciiz "Not Recognized"
.text

main:

li $v0, 8
la $a0, user_string   
li $a1, 1001
syscall
la $s0, user_string

jal strlength
addi $s1, $v0, 0
addi $s2, $v1, 0

move $a0,$s1
move $a1, $s0
jal removeleadspace
move $s0,$v0

move $a0,$s2
addi $a1, $s1, 0
jal removeendspace
addi $s2, $v0, 0

bgt $s1,4, invalidchars
addi $a0,$s1, 0
move $a1, $s0
li $a2, 31

jal string2deci
move $s3,$v0
li $v0,1
addi $a0,$s3,0
syscall